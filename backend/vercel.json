{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "DATABASE_URL": "sqlite:///tmp/curagenie.db",
    "SECRET_KEY": "@secret_key",
    "CORS_ORIGINS": "https://*.vercel.app",
    "DEBUG": "false"
  }
}
